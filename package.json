{
  "name": "ngx-cypress-test",
  "version": "4.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdobrynin/ngx-cypress-test.git"
  },
  "bugs": {
    "url": "https://github.com/vdobrynin/ngx-cypress-test/issues"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "run-script-os",
    "start:win32": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "start:darwin:linux": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-cypress-test",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "cy:verify": "cypress verify",
    "cy:run": "npx cypress run --headless --key aaa4020f-12a8-4919-bb58-f4b2e031907a",
    "cy:run_spec": "npx cypress run --spec 'cypress/integration/testWithPageObjects.spec.js' --headless --record --key aaa4020f-12a8-4919-bb58-f4b2e031907a --parallel",
    "cy:run_visual": "npx cypress run --spec 'cypress/integration/visualTest.spec.js' -b chrome",
    "cy:run_browser": "npm run cy:run_spec -- -b chrome --group chrome --ci-build-id 53 && npm run cy:run_spec -- -b firefox --group firefox --ci-build-id 53",
    "cy:run_browsers": "npm run cy:run_spec -- -b chrome --group chrome --ci-build-id &ID && npm run cy:run_spec -- -b firefox --group firefox --ci-build-id $ID",
    "cy:run_group": "ID=$(uuid) npm run cy:run_browsers",
    "cypress:e2e": "server-test start http-get://localhost:4200 cy:run"
  },
  "dependencies": {
    "@agm/core": "^3.0.0-beta.0",
    "@angular/animations": "^19.2.13",
    "@angular/cdk": "^19.2.17",
    "@angular/common": "^19.2.13",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^19.2.13",
    "@angular/forms": "^19.2.13",
    "@angular/platform-browser": "^19.2.13",
    "@angular/platform-browser-dynamic": "^19.2.13",
    "@angular/router": "^19.2.13",
    "@asymmetrik/ngx-leaflet": "3.0.1",
    "@nebular/auth": "^15.0.0",
    "@nebular/eva-icons": "^15.0.0",
    "@nebular/security": "^15.0.0",
    "@nebular/theme": "^15.0.0",
    "@swimlane/ngx-charts": "^22.0.0",
    "angular-tree-component": "8.5.6",
    "angular2-chartjs": "0.4.1",
    "angular2-toaster": "^11.0.1",
    "bootstrap": "^5.3.6",
    "chart.js": "^2.9.3",
    "ckeditor": "^4.12.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "echarts": "^5.6.0",
    "eva-icons": "^1.1.0",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "leaflet": "1.2.0",
    "nebular-icons": "1.1.0",
    "ng2-ckeditor": "^1.2.2",
    "ng2-completer": "2.0.8",
    "ng2-smart-table": "^1.7.2",
    "ngx-echarts": "^4.0.1",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.3.0",
    "sass": "^1.53.0",
    "socicon": "3.0.5",
    "tinymce": "^7.9.0",
    "tslib": "^1.9.0",
    "typeface-exo": "0.0.22",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.13",
    "@angular/cli": "^19.2.13",
    "@angular/compiler-cli": "^19.2.13",
    "@angular/language-service": "8.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@types/d3-color": "1.0.5",
    "@types/geojson": "7946.0.8",
    "@types/googlemaps": "^3.30.4",
    "@types/leaflet": "1.2.3",
    "@types/node": "6.0.90",
    "codelyzer": "^0.0.28",
    "conventional-changelog-cli": "^2.1.1",
    "cypress": "^14.4.0",
    "husky": "0.13.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "2.6.1",
    "run-script-os": "^1.1.6",
    "stylelint": "^16.19.1",
    "ts-node": "9.1.1",
    "tslint": "^6.1.3",
    "tslint-language-service": "^0.9.9",
    "typescript": "3.4.5"
  }
}

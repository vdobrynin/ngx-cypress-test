{
  "name": "ngx-admin",
  "version": "4.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akveo/ngx-admin.git"
  },
  "bugs": {
    "url": "https://github.com/akveo/ngx-admin/issues"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "run-script-os",
    "start:win32": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "start:darwin:linux": "export NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "cy:verify": "cypress verify",
    "cy:run_chrome": "npx cypress run --spec 'cypress/integration/testWithPageObjects.spec.js' --browser chrome",
    "cy:run_spec_old": "npx cypress run --spec 'cypress/integration/testWithPageObjects.spec.js' --headless",
    "cy:run": "npx cypress run --headless --record --key 1f689815-27e0-4704-ba3f-a229cbbbc527 --parallel",
    "cy:run_spec": "npx cypress run --spec 'cypress/integration/testWithPageObjects.spec.js' --headless --record --key 1f689815-27e0-4704-ba3f-a229cbbbc527",
    "cy:run_parallel": "npm run cy:run & npm run cy:run",
    "cy:run_firefox_1": "npx cypress run -s 'cypress/integration/testWithPageObjects.spec.js' --browser firefox",
    "cy:run_firefox_2": "npx cypress run -s 'cypress/integration/testWithPageObjects.spec.js' --browser firefox  --headed",
    "cy:run_edge": "npx cypress run -s 'cypress/integration/testWithPageObjects.spec.js' --browser edge",
    "cy:run_electron": "npx cypress run -s 'cypress/integration/testWithPageObjects.spec.js' --browser electron --headed",
    "cy:run_webkit_1": "npx cypress run -s 'cypress/integration/testWithPageObjects.spec.js' --browser webkit",
    "cy:run_webkit_2": "npx cypress run -s 'cypress/integration/testWithPageObjects.spec.js' --browser webkit --headed",
    "cy:run_browsers": "npm run cy:run_spec -- --browser firefox & npm run cy:run_spec -- --browser --edge & npm run cy:run_spec -- --browser --chrome",
    "cy:run_parallel_2browsers_old": "npm run cy:run_spec --spec --browser firefox --group Firefox --ci-build-id 1 & npm run cy:run_spec --spec --browser --chrome --group Chrome --ci-build-id 1",
    "cy:run_parallel_2browsers": "npm run cy:run_spec --spec --browser firefox --group Firefox --ci-build-id $ID & npm run cy:run_spec --spec --browser --chrome --group Chrome --ci-build-id $ID",
    "cy:run_group": "ID=$(uuid) npm run cy:run_browsers",
    "cypress:e2e": "server-test start http-get://localhost:4200 cy:run",
    "cy:run2_old": "npx cypress run --headless --record --key xxx --parallel --ci-build-id 1",
    "cy:run2": "npx cypress run --headless --record --key xxx --parallel --ci-build-id $ID",
    "cy:run_p": "npm run cy:run2 & npm run cy:run2",
    "cy:run_b": "ID=$(uuid) npm run cy:run_p"
  },
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@asymmetrik/ngx-leaflet": "3.0.1",
    "@nebular/auth": "4.1.2",
    "@nebular/eva-icons": "4.1.2",
    "@nebular/security": "4.1.2",
    "@nebular/theme": "4.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-tree-component": "8.5.6",
    "angular2-chartjs": "0.4.1",
    "angular2-toaster": "^7.0.0",
    "bootstrap": "4.3.1",
    "chart.js": "^2.9.3",
    "ckeditor": "4.7.3",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "cypress-plugin-snapshots": "^1.4.4",
    "echarts": "^4.0.2",
    "eva-icons": "^1.1.0",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "leaflet": "1.2.0",
    "nebular-icons": "1.1.0",
    "ng2-ckeditor": "^1.2.2",
    "ng2-completer": "2.0.8",
    "ng2-smart-table": "1.3.5",
    "ngx-echarts": "^4.0.1",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.3.0",
    "sass": "^1.53.0",
    "socicon": "3.0.5",
    "tinymce": "4.5.7",
    "tslib": "^1.9.0",
    "typeface-exo": "0.0.22",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.0.2",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "8.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@types/d3-color": "1.0.5",
    "@types/geojson": "7946.0.8",
    "@types/googlemaps": "^3.30.4",
    "@types/leaflet": "1.2.3",
    "@types/node": "6.0.90",
    "codelyzer": "^5.0.1",
    "conventional-changelog-cli": "^2.1.1",
    "cypress": "^13.11.0",
    "husky": "0.13.3",
    "npm-run-all": "4.0.2",
    "playwright-webkit": "1.34",
    "rimraf": "2.6.1",
    "run-script-os": "^1.1.6",
    "stylelint": "7.13.0",
    "ts-node": "9.1.1",
    "tslint": "^6.1.3",
    "tslint-language-service": "^0.9.9",
    "typescript": "3.4.5"
  }
}
